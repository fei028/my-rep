}
.page-control {
	width:100%;
	margin-bottom:0;
/* 	border:1px solid; */
	float:left;
}
.page-control .check-input {
	float:left;
	padding-left:10px;
	padding-top:25px;
}
.page-control .pageBtn{
	float:left;
	padding-top:20px;
	padding-left:30px;
}
.page-control .pageDiv {
	float:right;
	padding-right:25px;
}
.pageDiv  span {
	font-size:12px;
}
.pagination {
	padding-left:10px;
}
.pagination li.page-num {
	padding-left:20px;
}
.pagination li.page-num input {
	width:40px;	
}
