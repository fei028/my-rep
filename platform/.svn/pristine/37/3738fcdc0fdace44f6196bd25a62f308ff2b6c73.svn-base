.fixedbody {
	overflow:auto; 
}
.nav-path {
	width:100%;
	padding:0;
	margin:0;
}
.nav-path ul {
	padding:0;
	margin:0;
	line-height:40px;
	padding-left:22px;
	min-height:40px;
}
.nav-path ul  li{
	float:left;
}
.nav-breadcrumb > li + li:before {
	content:">";
	padding:0 5px;
}
.theme-condition {
	padding-top:20px;
	padding-bottom:20px;
}
.theme-condition label{
	font-size:14px;
	padding-right:8px;
	padding-left:20px;
	font-weight:normal;
}
.theme-content {
	padding-left:20px; 
	padding-right:20px;
}
.dvThead{
	border-top:1px solid #F7F7F7;
	border-left:1px solid #F7F7F7;
	border-right:1px solid #F7F7F7;
	margin-bottom:0;
}
.dvThead table{
	margin-bottom:0;
	padding-bottom:0;
}
.dvThead table thead th {
	border-bottom:none;
}
.dvTbody{
	border:1px solid #F7F7F7;
	border-bottom:none;
	border-top:none;
	height: 480px;
	overflow: auto;
	margin-top:0;
	margin-bottom:0;
}
.dvTbody table{
	margin-top:0;
	padding-top:0;
	margin-bottom:0;
	padding-bottom:0;
}
.dvTbody table tr:last-child{
	border-bottom:1px solid #F7F7F7;
}
